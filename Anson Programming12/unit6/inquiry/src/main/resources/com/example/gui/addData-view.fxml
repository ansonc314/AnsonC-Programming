<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="tableWindow" prefHeight="500.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.Controller_AddData">
   <children>
      <VBox fillWidth="false" prefHeight="500.0" prefWidth="300.0">
         <children>
            <ImageView fitHeight="82.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@sqLogo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <Label id="H3_label" prefHeight="17.0" prefWidth="302.0" stylesheets="@style.css" text="Weight" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="30.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newWeight" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <Label id="H3_label" prefHeight="17.0" prefWidth="302.0" stylesheets="@style.css" text="Systolic Blood Pressure" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newSBP" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <Label id="H3_label" layoutX="60.0" layoutY="287.0" prefHeight="17.0" prefWidth="302.0" stylesheets="@style.css" text="Diastolic Blood Pressure" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newDBP" layoutX="60.0" layoutY="304.0" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <ButtonBar opacity="0.54" prefHeight="40.0" prefWidth="246.0">
              <buttons>
                <Button id="Button" fx:id="addUserButton" mnemonicParsing="false" onAction="#submitAddDataForm" prefHeight="23.0" prefWidth="139.0" stylesheets="@style.css" text="Submit" />
                  <Button id="Button" fx:id="exitButton" mnemonicParsing="false" onAction="#exitAddDataForm" prefHeight="22.0" stylesheets="@style.css" text="Exit" textAlignment="CENTER" textFill="#000000b8" />
              </buttons>
               <effect>
                  <Glow />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="20.0" top="35.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
