<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="tableWindow" prefHeight="500.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.Controller_AddUser">
   <children>
      <VBox fillWidth="false" prefHeight="500.0" prefWidth="300.0">
         <children>
            <ImageView fitHeight="82.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@sqLogo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <Label prefHeight="17.0" prefWidth="302.0" text="New User Name" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="30.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newUserName" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <Label layoutX="60.0" layoutY="182.0" prefHeight="17.0" prefWidth="302.0" text="Password" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newPassword" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="17.0" prefWidth="302.0" text="Full Name" textFill="#f5ebeb">
               <VBox.margin>
                  <Insets left="50.0" top="20.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="newFullName" prefHeight="25.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </TextField>
            <ButtonBar opacity="0.54" prefHeight="40.0" prefWidth="252.0">
              <buttons>
                <Button id="loginButton" fx:id="addUserButton" mnemonicParsing="false" onAction="#submitAddUserForm" prefHeight="22.0" stylesheets="@style.css" text="Submit" />
                  <Button id="loginButton" fx:id="exitButton" mnemonicParsing="false" onAction="#exit" prefHeight="22.0" stylesheets="@style.css" text="Exit" textAlignment="CENTER" textFill="#000000b8" />
              </buttons>
               <effect>
                  <Glow />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
