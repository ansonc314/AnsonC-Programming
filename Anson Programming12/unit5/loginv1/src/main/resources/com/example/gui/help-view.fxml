<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="tableWindow" prefHeight="500.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.Controller_Help">
   <children>
      <VBox fillWidth="false" prefHeight="500.0" prefWidth="300.0">
         <children>
            <ImageView fitHeight="82.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@sqLogo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <Label id="help-view-label" prefHeight="96.0" prefWidth="220.0" text="If you have troubles, please contact our technical supports.  Their contact information is: help@xpertstudio.com" textFill="#f8f6f6" wrapText="true">
               <VBox.margin>
                  <Insets left="40.0" right="40.0" top="50.0" />
               </VBox.margin>
            </Label>
            <ButtonBar opacity="0.54" prefHeight="40.0" prefWidth="252.0">
              <buttons>
                  <Button id="loginButton" fx:id="exitButton" mnemonicParsing="false" onAction="#exit" prefHeight="22.0" prefWidth="203.0" stylesheets="@style.css" text="Exit" textAlignment="CENTER" textFill="#000000b8" />
              </buttons>
               <effect>
                  <Glow />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
