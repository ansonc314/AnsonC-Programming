<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="tableWindow" prefHeight="500.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.Controller_Exit">
   <children>
      <VBox fillWidth="false" prefHeight="500.0" prefWidth="300.0">
         <children>
            <ImageView fitHeight="82.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@sqLogo.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" top="20.0" />
               </VBox.margin>
            </ImageView>
            <Label id="exit-warning-view-label" prefHeight="17.0" prefWidth="172.0" text="Are you sure want to exit?" textAlignment="RIGHT" textFill="#f8f8f8">
               <VBox.margin>
                  <Insets bottom="50.0" left="60.0" right="40.0" top="80.0" />
               </VBox.margin>
               <font>
                  <Font size="34.0" />
               </font>
            </Label>
            <ButtonBar opacity="0.54" prefHeight="40.0" prefWidth="252.0">
              <buttons>
                  <Button id="loginButton" fx:id="exitWindow_exitButton" mnemonicParsing="false" onAction="#exit" prefHeight="22.0" prefWidth="203.0" stylesheets="@style.css" text="EXIT" textAlignment="CENTER" textFill="#000000b8">
                     <padding>
                        <Insets top="50.0" />
                     </padding></Button>
                  <Button id="loginButton" fx:id="exitWindow_cancelButton" mnemonicParsing="false" onAction="#cancel" text="CANCEL" />
              </buttons>
               <effect>
                  <Glow />
               </effect>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
